  "scripts": {
    "start": "yarn nx run-many --target=serve --projects=math,vocmem,phys,society --maxParallel=4 --parallel=true",
    "clean": "yarn build:clean && yarn dev:clean",
    "build": "yarn build:data && yarn build:vocmem && yarn build:phys && yarn build:math && yarn build:society",
    "build:clean": "rimraf dist",
    "build:data": "ts-node scripts/csv-to-json.ts",
    "build:full": "yarn build:data && yarn build",
    "build:math": "nx run math:build && nx run math:export",
    "build:phys": "nx run phys:build && nx run phys:export",
    "build:society": "nx run society:build && nx run society:export",
    "build:vocmem": "nx run vocmem:build && nx run vocmem:export",
    "deploy": "yarn build && yarn rsync",
    "rsync": "yarn rsync:phys && yarn rsync:vocmem && yarn rsync:math && yarn rsync:society",
    "rsync:math": "rsync -avz -e \"ssh -i key -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no -p 10022\" --delete ./dist/apps/math/exported/*   phasetr@sv935.xserver.jp:~/phasetr.com/public_html/service/math-gallery",
    "rsync:phys": "rsync -avz -e \"ssh -i key -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no -p 10022\" --delete ./dist/apps/phys/exported/*   phasetr@sv935.xserver.jp:~/phasetr.com/public_html/service/phys-gallery",
    "rsync:society": "rsync -avz -e \"ssh -i key -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no -p 10022\" --delete ./dist/apps/society/exported/* phasetr@sv935.xserver.jp:~/phasetr.com/public_html/service/society",
    "rsync:vocmem": "rsync -avz -e \"ssh -i key -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no -p 10022\" --delete ./dist/apps/vocmem/exported/* phasetr@sv935.xserver.jp:~/phasetr.com/public_html/service/vocmem",
    "test": "nx test",
    "dev:clean": "rimraf tmp",
    "preinstall": "npx typesync || :"
  },
